@prefix exl: <http://example.org/lexicon#> .
@prefix exo: <http://example.org/ontology#> .
@prefix lemon: <http://www.lemon-model.net/lemon#> .
@prefix lexinfo: <http://lexinfo.net/ontology/2.0/lexinfo#> .

exl:lexicon a lemon:Lexicon ;
    lemon:entry exl:mortal,
                exl:person,
                exl:Socrates .

exl:mortal a lemon:Word ;
    lexinfo:partOfSpeech lexinfo:noun .

exl:person a lemon:Word ;
    lexinfo:partOfSpeech lexinfo:noun .

exl:Socrates a lemon:LexicalEntry ;
    lexinfo:partOfSpeech lexinfo:properNoun .