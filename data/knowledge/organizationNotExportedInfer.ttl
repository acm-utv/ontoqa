###  http://www.semanticweb.org/organization#Company
:Company rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :hasHeadquarter ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Nation
                         ] .


###  http://www.semanticweb.org/organization#Nation
:Nation rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty [ owl:inverseOf :hasHeadquarter
                                             ] ;
                              owl:someValuesFrom [ rdf:type owl:Class ;
                                                   owl:unionOf ( :Company
                                                                 :Organization
                                                               )
                                                 ]
                            ] .


###  http://www.semanticweb.org/organization#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :hasHeadquarter ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Nation
                              ] .


###  http://www.semanticweb.org/organization#Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasNation ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Nation
                        ] .


