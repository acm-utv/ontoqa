<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        %date{HH:mm:ss.SSS} %-5level %logger %method - %msg%n
      </pattern>
    </encoder>
  </appender>

  <root level="ALL">
    <appender-ref ref="CONSOLE" />
  </root>

  <logger name="ch.qos.logback" level="OFF" />

</configuration>