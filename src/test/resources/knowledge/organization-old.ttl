@prefix : <http://www.ontoqa.com/organization#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.ontoqa.com/organization> .

<http://www.ontoqa.com/organization> rdf:type owl:Ontology .

#################################################################
#    Object Properties
#################################################################

###  http://www.ontoqa.com/organization#acquireCompany
:acquireCompany rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Organization ;
                rdfs:range :Company .


###  http://www.ontoqa.com/organization#isCEOOf
:isCEOOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :isCorporateOfficerOf ;
         rdfs:domain :CEO ;
         rdfs:range :Organization .


###  http://www.ontoqa.com/organization#isCFOOf
:isCFOOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :isCorporateOfficerOf ;
         rdfs:domain :CFO ;
         rdfs:range :Organization .


###  http://www.ontoqa.com/organization#isChairmanOf
:isChairmanOf rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Chairman ;
              rdfs:range :Organization .


###  http://www.ontoqa.com/organization#isCorporateOfficerOf
:isCorporateOfficerOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain :CorporateOfficer ;
                      rdfs:range :Organization .


###  http://www.ontoqa.com/organization#isFounderOf
:isFounderOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Founder ;
             rdfs:range :Organization .


###  http://www.ontoqa.com/organization#isHeadquartered
:isHeadquartered rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Company
                                             :Organization
                                           )
                             ] ;
                 rdfs:range :Nation .


###  http://www.ontoqa.com/organization#nationality
:nationality rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Person ;
             rdfs:range :Nation .


#################################################################
#    Data properties
#################################################################

###  http://www.ontoqa.com/organization#companyValue
:companyValue rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdfs:domain :Company ;
              rdfs:range xsd:double .


###  http://www.ontoqa.com/organization#netIncome
:netIncome rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain :Organization ;
           rdfs:range xsd:double ;
           rdfs:comment "The net income is in billion U.S. dollars"@en .


#################################################################
#    Classes
#################################################################

###  http://www.ontoqa.com/organization#CEO
:CEO rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :CorporateOfficer
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :isCEOOf ;
                                                  owl:someValuesFrom :Organization
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:subClassOf :CorporateOfficer .


###  http://www.ontoqa.com/organization#CFO
:CFO rdf:type owl:Class ;
     owl:equivalentClass [ owl:intersectionOf ( :CorporateOfficer
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :isCFOOf ;
                                                  owl:someValuesFrom :Organization
                                                ]
                                              ) ;
                           rdf:type owl:Class
                         ] ;
     rdfs:subClassOf :CorporateOfficer .


###  http://www.ontoqa.com/organization#Chairman
:Chairman rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Person
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :isChairmanOf ;
                                                       owl:someValuesFrom :Organization
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Person .


###  http://www.ontoqa.com/organization#Company
:Company rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty :isHeadquartered ;
                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Nation
                         ] .


###  http://www.ontoqa.com/organization#CorporateOfficer
:CorporateOfficer rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( :Person
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :isCorporateOfficerOf ;
                                                               owl:someValuesFrom :Organization
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf :Person .


###  http://www.ontoqa.com/organization#Founder
:Founder rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Person
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :isFounderOf ;
                                                      owl:someValuesFrom :Organization
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Person .


###  http://www.ontoqa.com/organization#Nation
:Nation rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty [ owl:inverseOf :isHeadquartered
                                             ] ;
                              owl:someValuesFrom [ rdf:type owl:Class ;
                                                   owl:unionOf ( :Company
                                                                 :Organization
                                                               )
                                                 ]
                            ] .


###  http://www.ontoqa.com/organization#Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :isHeadquartered ;
                                owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :Nation
                              ] .


###  http://www.ontoqa.com/organization#Person
:Person rdf:type owl:Class .


